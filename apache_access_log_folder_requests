#This version of the script is based on an IP as the URL, it uses ':80' as the first point to cut from in AWK 
#A new version will be released once the domain name has been added

#The * after access.log will parse all access.log files in the directory
#This will attempt to parse GZ files, be mindful of this when checking results

cat /var/log/apache2/access.log* | awk -F":80" '{print $2}' |  awk -F" " '{print $1}' | sort | uniq -c | sort -nr
